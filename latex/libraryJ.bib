eprint = {NIHMS150003},
file = {:home/jrothschild/Research/Papers to read/Biofilms/ScienceDirect{\_}articles{\_}23Mar2018{\_}18-55-42.068/Modeling-the-response-of-a-biofilm-to-silver-based-{\_}2013{\_}Mathematical-Biosci.pdf:pdf},
