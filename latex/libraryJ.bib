isbn = {0025-5564},
issn = {18793134},
journal = {Mathematical Biosciences},
keywords = {Approximations of moments,Birth-death process,Cumulant truncation,Extended logistic growth family,Global population dynamics database,Quasi-equilibrium distribution},
pages = {96--112},
publisher = {Elsevier Inc.},
title = {{A simple approximation of moments of the quasi-equilibrium distribution of an extended stochastic theta-logistic model with non-integer powers}},
url = {http://dx.doi.org/10.1016/j.mbs.2015.10.017},
