isbn = {1549-3296 (Print)$\backslash$r1549-3296 (Linking)},
issn = {15493296},
journal = {Journal of Biomedical Materials Research Part A},
keywords = {colloidal drug,functional groups,nanoparticles,protein ad-,sorption,two-dimensional electrophoresis},
number = {3},
