issn = {19432631},
journal = {Genetics},
keywords = {Cancer,Moran process,Population genetics,Stochastic modeling,WKB method},
number = {4},
pages = {1213--1228},
pmid = {25624316},
title = {{Stochastic tunneling and metastable states during the somatic evolution of cancer}},
volume = {199},
year = {2015}
}
