file = {:home/jrothschild/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Holling - 1973 - RESILIENCE AND STABILITY OF ECOLOGICAL SYSTEMS.pdf:pdf},
isbn = {00664162},
