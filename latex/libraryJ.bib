title = {{On the distribution of DNA translocation times in solid-state nanopores: an analysis using Schr{\"{o}}dinger's first-passage-time theory.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23963318},
volume = {25},
year = {2013}
}
