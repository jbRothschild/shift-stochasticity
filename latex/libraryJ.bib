file = {:home/jrothschild/Research/Papers to read/Mechanics-and-dynamics-of-reconstit{\_}2015{\_}Biochimica-et-Biophysica-Acta--BBA-.pdf:pdf},
isbn = {0167-4889},
issn = {18792596},
journal = {Biochimica et Biophysica Acta - Molecular Cell Research},
keywords = {Actin,Active soft matter,Biopolymer rheology,Composite networks,Cytoskeleton,Dynamic arrest},
number = {11},
pages = {3038--3042},
pmid = {26130089},
publisher = {Elsevier B.V.},
title = {{Mechanics and dynamics of reconstituted cytoskeletal systems}},
